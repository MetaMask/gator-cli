{
  "name": "gator",
  "version": "0.1.0",
  "private": true,
  "description": "ERC-7710 Delegation CLI â€” grant, redeem, and revoke permissions on MetaMask Smart Accounts",
  "repository": {
    "type": "git",
    "url": "https://github.com/smartgator/gator-cli.git"
  },
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist && yarn build",
    "build:docs": "typedoc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "format": "prettier '**/*.json' '**/*.md' '**/*.yml' '**/*.mjs' '**/*.mts' '**/*.ts' --ignore-path .gitignore --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.json' '**/*.md' '**/*.yml' '**/*.mjs' '**/*.mts' '**/*.ts' --ignore-path .gitignore --no-error-on-unmatched-pattern --check",
    "prepack": "./scripts/prepack.sh",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@metamask/smart-accounts-kit": "^0.3.0",
    "commander": "^12.1.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.8",
    "tsx": "^4.19.0",
    "typedoc": "^0.26.11",
    "typescript": "~5.7.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": "^20 || ^22 || >=24"
  }
}
