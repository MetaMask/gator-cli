{
  "name": "@metamask/gator-cli",
  "version": "0.1.0-alpha.0",
  "private": true,
  "description": "CLI for managing ERC-7710 delegations using MetaMask Smart Accounts Kit â€” grant, redeem, revoke, and inspect permissions from the terminal",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/gator-cli.git"
  },
  "license": "(Apache-2.0 OR MIT)",
  "type": "module",
  "bin": {
    "gator": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist && yarn build",
    "build:docs": "typedoc",
    "changelog:update": "auto-changelog update --prettier",
    "changelog:validate": "auto-changelog validate --prettier",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "format": "prettier '**/*.json' '**/*.md' '**/*.yml' '**/*.mjs' '**/*.mts' '**/*.ts' --ignore-path .gitignore --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.json' '**/*.md' '**/*.yml' '**/*.mjs' '**/*.mts' '**/*.ts' --ignore-path .gitignore --no-error-on-unmatched-pattern --check",
    "prepack": "./scripts/prepack.sh",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@metamask/smart-accounts-kit": "^0.3.0",
    "commander": "^12.1.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^5.0.2",
    "@types/node": "^22.0.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.8",
    "tsx": "^4.19.0",
    "typedoc": "^0.26.11",
    "typescript": "~5.7.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": "^20 || ^22 || >=24"
  }
}
